<script>
	var videoplayer = $("._j_videoplayer").get(0),
		userAgent = navigator.userAgent
		isSafari = userAgent.indexOf('Chrome') == -1 && userAgent.indexOf('Safari') > -1,
		goUsaLayer = $('.gousa');
	goUsaLayer.delegate('a', 'click', function (ev) {
		var target = $(ev.currentTarget);
		if (target.hasClass('videostop')) {
			videoplayer.pause();
			target.removeClass('videostop');
		} else if(target.hasClass('videoplay')){
			videoplayer.play();
			target.addClass('videostop');
		} else if(target.hasClass('voiceoff')){
			videoplayer.volume = 1;
			target.removeClass('voiceoff');
		} else if(target.hasClass('voiceon')){
			videoplayer.volume = 0;
			target.addClass('voiceoff');
		}
	}).bind('mouseover', function(ev){
		$(ev.currentTarget).addClass('videohover');
	}).bind('mouseout', function(ev){
		$(ev.currentTarget).removeClass('videohover');
	});

	if(isSafari){
		goUsaLayer.addClass('videohover');
		$('.videoplay').removeClass('videostop');
	}
</script>
        
        <div id="pagelet-block-23e316e5140a77b0b00f1bda2831c170" class="pagelet-block" data-api=":mdd:pagelet:salesGonglveApi" data-params="{&quot;iMddid&quot;:&quot;11099&quot;}" data-async="1" data-controller="">    <div class="row row-local row-bg" data-cs-t="目的地详情页自由行攻略">
        <div class="wrapper" data-cs-p="">
            <h2 class="r-title">自由行攻略</h2>
            <div class="local-pro">
                <ul class="clearfix">
                                            <li>
                            <a class="img" href="http://www.mafengwo.cn/gonglve/ziyouxing/1262.html" target="_blank">
                                <img src="https://n1-q.mafengwo.net/s9/M00/88/98/wKgBs1hOC8KAayDmAAHEmCFanAM05.jpeg?imageMogr2%2Fthumbnail%2F%21320x170r%2Fgravity%2FCenter%2Fcrop%2F%21320x170%2Fquality%2F100" width="320" height="170">
                                                                    <span class="mark">3人体验过</span>
                                                            </a>
                            <a class="title" href="http://www.mafengwo.cn/gonglve/ziyouxing/1262.html" target="_blank">不一样的纸醉金迷，揭秘阿姆斯特丹红灯区</a>
