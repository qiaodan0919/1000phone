            amap = {"aMarks":[{"mddname":"\u5e15\u52b3","left":"294px","top":"102px","iconcolor":"icon-a","iconfont":"false","mddid":15911},{"mddname":"\u83f2\u5f8b\u5bbe","left":"0px","top":"36px","iconcolor":"icon-b","iconfont":"false","mddid":10067},{"mddname":"\u5370\u5ea6\u5c3c\u897f\u4e9a","left":"22px","top":"334px","iconcolor":"icon-b","iconfont":"false","mddid":10181},{"mddname":"\u5173\u5c9b","left":"461px","top":"8px","iconcolor":"icon-b","iconfont":"true","mddid":19016}],"sTop":"32px","sLeft":"182px","iMddid":15911};
    function setPageletBg() {
        var rows = $('#container >.row,#container .pagelet-block >.row'),
                pos = 0, i, len;
        for (i = 0, len = rows.length; i < len; i++) {
            $(rows[i])[i % 2 == 0 ? 'addClass' : 'removeClass']('row-bg');
        }
    }
    setPageletBg();
    M.Event.on('pagelet loaded', function(data){
        setPageletBg();
        data.pagelet.container.find('img.lazy').lazyload({
            threshold : 400
        });
    });
</script>
