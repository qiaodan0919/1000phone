            ajax_lock;
        $sorts.on('click', function (e, styleOnly) {
            e.preventDefault();
            var $this = $(this), $tab = $this.closest('.tab-item,.tn-dropdown'),
                $placeholder = $tab.find('.dropdown-trigger span'),
                changed;
            sort = $this.data('sort');
            if (!$this.is('.on')) {
                $tab.addClass('on').siblings().removeClass('on');
                $sorts.removeClass('on');
                $this.addClass('on');
                $placeholder.text($this.text());
                changed = true;
            }
            if (changed && !styleOnly) {
                if ($this.data('sort') != 1) {
                    $filters.filter(':nth-child(1)').trigger('click', [1]);
                    $tags.filter('.on').trigger('click', [1]);
                }
                getTravelNotes();
            }
            styleOnly && e.stopPropagation();
        });
        $filters.on('click', function (e, styleOnly) {
            e.preventDefault();
            var $this = $(this), $dropdown = $this.closest('.tn-dropdown'),
                $placeholder = $dropdown.find('.dropdown-trigger span'),
                changed,
                $link = $this.find('>a'),
                params = $link.attr('href');
            if (params) {
                params = params.split('/');
                params = params[params.length - 1].split('.')[0].split('-');
                if ($link.data('group') == 1) {
                    month = params[1];
                } else if ($link.data('group') == 2) {
                    cost = params[2];
                } else if ($link.data('group') == 3) {
                    days = params[3];
                }
            }

            if (!$this.is('.on')) {
                $filters.removeClass('on');
                $this.addClass('on');
                $placeholder.text($.trim($this.text()) == '不限' ? ($placeholder.data('placeholder') || $this.text()) : $this.text());
                changed = true;
            }
            if (changed && !styleOnly) {
                //只能允许最新游记方式排序
                $sorts.filter('[data-sort=1]').trigger('click', [1]);
                $tags.filter('.on').trigger('click', [1]);
                getTravelNotes();
            }
            styleOnly && e.stopPropagation();
        });

        $tags.click(function (e, styleOnly) {
            e.preventDefault();
            var $this = $(this);
            if ($this.is('.on')) {
                $this.removeClass('on');
                $tagTop.hide();
                tag_id = 0;
            } else {
                $this.addClass('on').siblings().removeClass('on');
                $tagTop.show().find('dt').text($this.find('.v-middle').text())
                    .end().find('dd').text($this.find('._desc').text());
                tag_id = $this.find('.v-middle').data('id');
            }
            if (!styleOnly) {
                //只能允许最新游记方式排序
                $sorts.filter('[data-sort=1]').trigger('click', [1]);
                $filters.filter(':nth-child(1)').trigger('click', [1]);
                getTravelNotes();
                            }
            styleOnly && e.stopPropagation();
        });

        $pagebar.on('click', 'a[href]', function (e) {
            if ('mdd_yj' == 'mdd_index') {
                e.preventDefault();
                var params = $(this).attr('href').split('/');
                params = params[params.length - 1].split('.')[0].split('-');
                $('html,body').animate({
                    scrollTop: ('mdd_yj' == 'mdd_index' ? $con.closest('.row-travels') : $con).offset().top - 60
                }, 500);
                getTravelNotes(params[0] == 's' ? params[5] : params[params.length - 1]);
            }
        });

        //目的地游记ajax翻页
        function getTravelNotes(page) {
            if (!ajax_lock) {
                ajax_lock = true;
                $.ajax({
                    url: "/gonglve/ajax.php?act=get_travellist",
                    aysn: false,
                    data: {
                        mddid:16827,
                        pageid: 'mdd_yj',
                        sort: sort,
                        cost: cost,
                        days: days,
                        month: month,
                        tagid: tag_id,
                        page: page || 1
                    },
                    dataType: "json",
                    type: "post",
                    success: function (data) {
                        $notelist.html(data.list).find("img.lazy").lazyload({
                            threshold: 400
                        });
                        $pagebar.html(data.page);
                        $(window).resize();
                    },
                    complete: function () {
                        ajax_lock = false;
                    }
                });
            }
        }

        //POI游记ajax翻页
        window.flip_over = function (poiid, page) {
            if (!ajax_lock) {
                ajax_lock = true;
                $.get('/gonglve/ajax.php', {
                    act: 'get_new_travellist', page: page, poi_id: poiid
                }, function (d) {
                    var $list = $(d.list);
                    if (d.msg == 'succ') {
                        $('.post-list').replaceWith($list);
                        $('.page-hotel').replaceWith(d.page);
                        $list.find("img.lazy").lazyload({
                            threshold: 400
                        });
                        $('html,body').animate({
                            scrollTop: $('.m-post').offset().top - 60
                        }, 500);
                    }
                    ajax_lock = false;
                }, 'json');
            }
        }
    })();
</script>
    </div>

    </div>
    <div class="p-aside">
