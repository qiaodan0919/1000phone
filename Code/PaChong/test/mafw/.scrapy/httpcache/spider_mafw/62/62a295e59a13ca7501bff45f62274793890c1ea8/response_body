<script language="JavaScript" type="text/javascript">
var poi_list = [];
var mddid = parseInt('16827');
var mdd_center = { lat:parseFloat('-4.666767433659'), lng:parseFloat('55.475463867188'), zoom:parseInt('2') },map;
