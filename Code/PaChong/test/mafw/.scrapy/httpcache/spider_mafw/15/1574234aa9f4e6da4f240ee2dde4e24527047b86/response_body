                'thumb_box' : '.dot-nav',  //ç¼©ç•¥å›¾ï¼Œæ²¡æœ‰è¯·ç½®ç©º
                'prev_btn' : '.book-prev',   //å‰ä¸€å¼ ï¼Œæ²¡æœ‰è¯·ç½®ç©º
                'next_btn' : '.book-next',   //ä¸‹ä¸€å¼ ï¼Œæ²¡æœ‰è¯·ç½®ç©º
                'thumb_focus_class' : 'on',  // å½“å‰æ­£åœ¨æ˜¾ç¤ºçš„å¹»ç¯å¯¹åº”çš„ç¼©ç•¥å›¾çš„class
                'auto_play' : true,  // è‡ªåŠ¨æ’­æ”¾
                'interval' : 8,  // å¹»ç¯åˆ‡æ¢æ—¶é—´é—´éš”
                'btn_auto_hide' : true,  // è‡ªåŠ¨éšè—ä¸Šä¸€å¼ ï¼Œä¸‹ä¸€å¼ æŒ‰é’®
                'contain' : true, //thumb_boxæ˜¯å¦æ˜¯$('#slide_box')çš„å­å…ƒç´ ,é™¤ç‰¹æ®Šæƒ…å†µåº”è¯¥ä¸ºtrue
                'always_play' : true
            });
            $('.sales-slide').mfwSlide({
                'width' : 260,
                'height': 180,
                'speed' : 300,  //æ»šåŠ¨é€Ÿåº¦
                'item' : 'ul>li', //
                'thumb_box' : '.dot-nav',  //ç¼©ç•¥å›¾ï¼Œæ²¡æœ‰è¯·ç½®ç©º
                'prev_btn' : '.sales-prev',   //å‰ä¸€å¼ ï¼Œæ²¡æœ‰è¯·ç½®ç©º
                'next_btn' : '.sales-next',   //ä¸‹ä¸€å¼ ï¼Œæ²¡æœ‰è¯·ç½®ç©º
                'thumb_focus_class' : 'on',  // å½“å‰æ­£åœ¨æ˜¾ç¤ºçš„å¹»ç¯å¯¹åº”çš„ç¼©ç•¥å›¾çš„class
                'auto_play' : true,  // è‡ªåŠ¨æ’­æ”¾
                'interval' : 8,  // å¹»ç¯åˆ‡æ¢æ—¶é—´é—´éš”
                'btn_auto_hide' : true,  // è‡ªåŠ¨éšè—ä¸Šä¸€å¼ ï¼Œä¸‹ä¸€å¼ æŒ‰é’®
                'contain' : true, //thumb_boxæ˜¯å¦æ˜¯$('#slide_box')çš„å­å…ƒç´ ,é™¤ç‰¹æ®Šæƒ…å†µåº”è¯¥ä¸ºtrue
