<style type="text/css">
	.map-mark {
		width: 24px;
		height: 28px;
		background: url(http://images.mafengwo.net/images/mdd/new_img/hotel-maps-bg11.png) no-repeat -35px 0;
		overflow: hidden;
		color: #fff;
		font-size: 14px;
		font-weight: bold;
		text-align: center;
		line-height: 22px;
		cursor: pointer;
		position: relative;
	}
	.map-mark.on {
		background-position: -35px -35px;
		z-index: 1009;
	}
</style>
