<script language="javascript" src="https://js.mafengwo.net/js/cv/js+jquery.mfwSlide:js+MouseTip^YFU^1493174420.js" type="text/javascript" crossorigin="anonymous"></script>


    <script language="JavaScript" type="text/javascript">
        $(function(){
            /*
             $('.more-mdd-desc').click(function(){
             var self = $(this);
             self.closest('p').hide();
             $('.t-info-more').fadeIn();
             self.closest('.bd').css('height','auto');
             });
             */
            /*宝藏提示*/
            $("#_js_treasureTips .i_know").click(function(e){
                e.preventDefault();
                $("#_js_treasureTips").hide();
            });
            $("._j_hover").hover(function(){
                $(".m-hotAsk ul li").removeClass("on");
                $(this).addClass('on');
            },function(){
                $(".m-hotAsk ul li").removeClass("on");
            });
            $('.book-slide').mfwSlide({
                'width' : 260,
                'height': 180,
                'speed' : 300,  //滚动速度
                'item' : 'ul>li', //
                'thumb_box' : '.dot-nav',  //缩略图，没有请置空
                'prev_btn' : '.book-prev',   //前一张，没有请置空
                'next_btn' : '.book-next',   //下一张，没有请置空
                'thumb_focus_class' : 'on',  // 当前正在显示的幻灯对应的缩略图的class
                'auto_play' : true,  // 自动播放
                'interval' : 8,  // 幻灯切换时间间隔
                'btn_auto_hide' : true,  // 自动隐藏上一张，下一张按钮
                'contain' : true, //thumb_box是否是$('#slide_box')的子元素,除特殊情况应该为true
                'always_play' : true
            });
            $('.sales-slide').mfwSlide({
                'width' : 260,
                'height': 180,
                'speed' : 300,  //滚动速度
                'item' : 'ul>li', //
                'thumb_box' : '.dot-nav',  //缩略图，没有请置空
                'prev_btn' : '.sales-prev',   //前一张，没有请置空
                'next_btn' : '.sales-next',   //下一张，没有请置空
                'thumb_focus_class' : 'on',  // 当前正在显示的幻灯对应的缩略图的class
                'auto_play' : true,  // 自动播放
                'interval' : 8,  // 幻灯切换时间间隔
                'btn_auto_hide' : true,  // 自动隐藏上一张，下一张按钮
                'contain' : true, //thumb_box是否是$('#slide_box')的子元素,除特殊情况应该为true
                'always_play' : true
            });
            $('.hover-cover').hover(function(){
                $(this).find('.img-cover').show();
            },function(){
                $(this).find('.img-cover').hide();
            });
            // 最新动态实时获取最新数据
            /*
             var actionList = $("#_j_new_action_list"),
             newsList = $([]);
             viewOnActionList = true,
             mddId = $('#_j_mddid').val(),
             lastId = actionList.children("li").first().data("action_id");
             setInterval(function() {
             if(M.windowFocused && viewOnActionList) {
             $.get("/gonglve/ajax_latest_comment_list.php", {"last_id":lastId, "mddid":mddId}, function(data) {
             data = $.parseJSON(data);
             if(data && data.payload && data.payload.html) {
             var newItems = $(data.payload.html).filter("li");
             newsList = newItems.add(newsList);
             lastId = newsList.first().data("action_id");
             }
             if(newsList.length > 0) {
             var targetItem = $(Array.prototype.pop.call(newsList));
             targetItem.css("display", "none");
             targetItem.prependTo(actionList);
             targetItem.slideDown();
             }
             });
             }
             }, 5000);
             */

            // 假动态滚动
            var actionList = $("#_j_new_action_list");
            if(actionList.children('li').length > 5) {
                setInterval(function() {
                    var lastItem = actionList.children('li').last().remove();
                    lastItem.css('display', 'none');
                    lastItem.prependTo(actionList).slideDown();
                }, 5000);
            }
            //mouse tip
            new MouseTip({
                'selector': '.impress-tip',
                'createTip': function(target) {
                    var title = target.attr('title');
                    //.data('title', target.attr('title'));
                    target.removeAttr('title');
                    return $('<p class="rec-tips" style="display:none; position:absolute">'+title+'</p>').appendTo('body');
                }
            });

            // 问答模块相关
            $("body").delegate("._j_question_item","mouseenter",function(event){
                $(this).addClass("hover");
            });

            $("body").delegate("._j_question_item","mouseleave",function(event){
                $(this).removeClass("hover");
            });

            $("body").delegate("._j_question_item","click",function(event){
                if (!$(event.target).hasClass("_j_filter_click")){
                    var qid = $(this).data("qid");
                    window.location.href = '/wenda/detail-' + qid + ".html";
                }
            });

            var suggest_timer;
            $("body").delegate("._j_suggest","focus",function(event){
                var target = $(this);
                var p = target.parents("._j_suggest_hd");
                p.addClass("focus");
                if (target.val() == suggest_default){
                    target.val('');
                }
                if (target.val() == ''){
                    var mddid = parseInt(target.data("mddid"),10);
                    if (mddid){
                        target.trigger("keyup");
                    }
                }
            });
            $("body").delegate("._j_suggest","blur",function(event){
                var target = $(this);
                var p = target.parents("._j_suggest_hd");
                p.removeClass("focus");
                if (target.val() == ''){
                    target.val(suggest_default);
                }
                suggest_timer = setTimeout(function(){
                    $("._j_suggest_box").hide();
                },500);
            });

            $("body").delegate("._j_suggest","keyup",function(event){
                if (suggest_timer){
                    clearTimeout(suggest_timer);
                }
                var target = $(this);
                var mddid = parseInt(target.data("mddid"),10);

                var title = $.trim(target.val());
                if (title.length == 0 && mddid == 0){
                    $("._j_suggest_box").hide();
                    return false;
                }
                $.get('/qa/ajax_tag.php', {'action' : 'suggest', 'mddid' : mddid, 'title' : title}, function(response) {
                    response = $.parseJSON(response);
                    if (response.payload && response.payload.html != ''){
                        $("._j_suggest_box").html(response.payload.html).show();
                    }
                });
            });

            $("body").delegate("._j_suggest","keydown",function(event){
                if (event.keyCode == 13){
                    $("._j_search").trigger("click");
                }
            });
            $("body").delegate("._j_search","click",function(event){
                event.preventDefault();
                var kw = $("._j_suggest").val();
                if (kw == suggest_default || kw.length == 0){
                    return false;
                }
                var mddname = $("._j_qa_search_box").attr("mddname");
                window.location.href = "/qa/list/search?key="+encodeURIComponent(mddname)+" "+encodeURIComponent(kw);
            });
        });
    </script>


            </div>
    </div>
