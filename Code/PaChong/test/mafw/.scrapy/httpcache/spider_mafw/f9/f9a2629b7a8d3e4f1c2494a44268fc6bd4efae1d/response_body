    <div id="pagelet-block-fb2380eeeefddf87ddde01d6053b2f48" class="pagelet-block" data-api=":mdd:pagelet:communityApi" data-params="{&quot;iMddid&quot;:&quot;24945&quot;}" data-async="1" data-controller="">        <div class="row row-ask row-bg">
