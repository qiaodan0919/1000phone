                'contain' : true, //thumb_boxÊòØÂê¶ÊòØ$('#slide_box')ÁöÑÂ≠êÂÖÉÁ¥†,Èô§ÁâπÊÆäÊÉÖÂÜµÂ∫îËØ•‰∏∫true
                'always_play' : true
            });
            $('.hover-cover').hover(function(){
                $(this).find('.img-cover').show();
            },function(){
                $(this).find('.img-cover').hide();
            });
            // ÊúÄÊñ∞Âä®ÊÄÅÂÆûÊó∂Ëé∑ÂèñÊúÄÊñ∞Êï∞ÊçÆ
            /*
