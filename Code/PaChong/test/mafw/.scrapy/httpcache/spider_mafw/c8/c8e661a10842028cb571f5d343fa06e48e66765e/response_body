<script language="javascript" src="https://js.mafengwo.net/js/google2amap.js?1493174420" type="text/javascript" crossorigin="anonymous"></script>

<script language="JavaScript" type="text/javascript">
var poi_list = [];
var mddid = parseInt('11099');
var mdd_center = { lat:parseFloat('52.132598876953'), lng:parseFloat('5.2912697792053'), zoom:parseInt('2') },map;
var marker_handle;
var infowin_hanlde;
var markers = { };
var currPop;
var allPop = { };
var currOtherPop;
var allOtherPop = { };
var mode, curType = 3, curPage = 1, pageSize = 10, totalPage, stackPage = [];
$(window).on('resize', function () {
	init_mapsize();//重绘页面
});
$(function(){
	//更改poi类型
	$('.change-poitype').click(function(){
		var self = $(this);
		var type_id = self.data('id')*1;
		if(type_id>0){
			$('.change-poitype').closest('li.on').removeClass('on');
			self.closest('li').addClass('on');
			curType = type_id;
			reload_map(1);
			$('.poi-mList').scrollTop(0);
		}
	});
	//关闭
	$('.btn-close').click(function(){
		$('body').css('overflow','auto');
		$('#mdd_map').fadeOut();
	});
	//上一页
	$('.btn-prev').click(function(){
		if($(this).hasClass('disable')) return;
		var testpage = curPage - 1;
		if(testpage>1){
			curPage = testpage;
		} else {
			curPage = 1;
			if(!$(this).hasClass('disable')){
				$(this).addClass('disable');
			}
		}
		if(curPage<totalPage && $('.btn-next').hasClass('disable')){
			$('.btn-next').removeClass('disable');
		}
		trun_page(curPage);
	});
	//下一页
	$('.btn-next').click(function(){
		if($(this).hasClass('disable')) return;
		var testpage = curPage + 1;
		if(testpage>totalPage){
			if(!$(this).hasClass('disable')){
				$(this).addClass('disable');
			}
		} else {
			curPage = curPage + 1;
			if(curPage==totalPage){
				$(this).addClass('disable');
			}
		}
		if(curPage>1 && $('.btn-prev').hasClass('disable')){
			$('.btn-prev').removeClass('disable');
		}
		trun_page(curPage);
	});
	//展开目的地地图
    var mapload;
	$('.show-mdd-map').click(function(){
        $('body').css('overflow','hidden');
		$('#mdd_map').show();
        if(!mapload){
            mapload = new MfwMapLoad(function() {
                if (typeof google != "object" || typeof google.maps != "object") {
                    $("#map_canvas").html('<div style="font-size: 1.5em; line-height: 3em; padding: 1em;">'
                    + '糟糕！您的网络不能顺利地连接<a href="http://ditu.google.cn" target="_blank">Google地图</a>，暂时不能使用此功能！'
                    + '<br><small>您可以稍后再试或者向您的网络提供商咨询相关情况！</small></div>');
                    return;
                }
                marker_handle = MarkerOverlay();
                infowin_hanlde = InfoWindowOverlay();
                get_data(0,function(d){
                    if(mode==1){ //目的地模式
                        $('.mdd-mode').show();
                    } else { //poi模式
                        $('.poi-mode').show();
                    }
                    map_draw('map_canvas', mdd_center, list_slice(poi_list,1), d.kmz);
                    init_mapsize();
                });
            }, 5000, false);
            mapload.autoLoadMap();
        }else if(typeof google == "object" && typeof google.maps == "object"){
            get_data(0,function(d){
                if(mode==1){ //目的地模式
                    $('.mdd-mode').show();
                } else { //poi模式
                    $('.poi-mode').show();
                }
                map_draw('map_canvas', mdd_center, list_slice(poi_list,1), d.kmz);
                init_mapsize();
            });
        }
	});
});
/* all functions */
//重载目的地地图
function reload_map(page){
	if(page==1){
		$('.btn-prev').addClass('disable');
		$('.btn-next').removeClass('disable');
	}
	get_data(page,function(d){
		//console.log('reload-'+stackPage.join(','))
		//console.log(poi_list.length);
		if(poi_list.length>0){
			map_draw('map_canvas', mdd_center, list_slice(poi_list,1));
		} else {
			$('#map_box').hide();
		}
	});
}
//翻页
function trun_page(page){
	var offset = $.inArray(page,stackPage);
	if(offset!=-1){
		var tmp = list_slice(poi_list,offset+1);
		if(tmp == false){
			no_result();
		} else {
			map_draw('map_canvas', mdd_center, tmp);
		}
	} else {
		reload_map(page);
	}
	$('.poi-mList').scrollTop(0);
	//console.log(page+'-'+poi_list.length);
}
//无数据
function no_result(){
	init_mapsize();
	map_draw('map_canvas', mdd_center,[]);
	$('#poiList').empty().append('<ul style="text-align: center;padding-top: 10px;">对不起，荷兰暂时还没有此类信息</ul>');
}
//将数拆分成子页
function list_slice(list,page){
	var offset = (page-1) * pageSize;
	if(list.length<offset){
		return [];
	} else {
		var last = offset+pageSize;
		last = last>list.length ? list.length : last;
		var new_list = [];
		for(var i=offset;i<last;i++){
			new_list.push(list[i]);
		}
		return new_list;
	}
}
//获取数据
function get_data(page,call_back){
	page = !page ? 1 : page;
	$.get('/gonglve/sg_ajax.php',{
			sAct:'getMapData',
			iMddid:11099,
		iType:curType,
		iPage:page
},function(d){
	if(d.ret==1) {
		mode = d.mode;
		poi_list = d.list;
		curPage = page;
		if(d.total > 0){
			totalPage = Math.ceil(d.total/10);
			stackPage = [];
			var last = d.mode==2 ? 3 : totalPage;
			for(var i=0;i<last;i++){
				if(page+i<=totalPage){
					stackPage.push(page+i);
				}
			}
			if(totalPage==1) $('.btn-next').addClass('disable');
			call_back(d);
		} else {
			totalPage = 1;
			$('.btn-next').addClass('disable');
			no_result();
		}
	}
	else alert('数据载入出错 '+ d.msg);
},'json');
}
//初始化地图浮层尺寸
function init_mapsize(){
	var win_height = $(window).height();
	var poilist_height = win_height-$('.hm-bar>.hd').outerHeight(true)-$('.hm-bar>.ft').outerHeight(true);
	$('.poi-mList').height(poilist_height);
	$('#map_canvas').height(win_height-$('.hm-cont>.hd').outerHeight(true));
	$('#map_canvas').width($(window).width()-$('.hm-bar').outerWidth(true));
}
//绘制地图上的数据
function map_draw(box, mdd_center, poi_list, kmz) {
	try{
		//地图
		var myLatLng = new google.maps.LatLng(mdd_center.lat, mdd_center.lng);
		var myOptions = {
			zoom : mdd_center.zoom,
			center : myLatLng,
			panControl : true,
			mapTypeId: google.maps.MapTypeId.ROADMAP
		};
		map = new google.maps.Map(document.getElementById(box), myOptions);
		if(kmz!=''){
			var areaborder = new google.maps.KmlLayer(kmz, {
				clickable:false
				//preserveViewport:mdd.preserveViewport
			});
			areaborder.setMap(map);
		}
		if(poi_list.length==0) return;
		var bounds = new google.maps.LatLngBounds();
		var pagesize = poi_list.length>10 ? 10 : poi_list.length;
		var order = 0;
		var captions = [{ },
			{
				rank:'人去过',
				num_comment:'个旅行地',
				link:'/travel-scenic-spot/mafengwo/'
			},
			{
				rank:'分',
				num_comment:'点评',
				link:'/poi/'
			},
		];
		//数据清空
		$('#poiList').empty();
		markers = { };
		allPop = { };
		allOtherPop = { };
		for(var i = 0; i < pagesize; i++){
			var p = poi_list[i];
			var poi_id = p.id;
			var latlng = new google.maps.LatLng(parseFloat(p.lat), parseFloat(p.lng));
			bounds.extend(latlng);
			var order = order + 1;
			$('#poiList').append('<li id="poi_'+poi_id+'" data-id="'+poi_id+'" class="item">\
                    <span class="p-img"><a href="javascript:void(0);"><img src="'+ p.img_link +'" width="110" height="70"><em>'+order+'</em></a></span>\
                    <h3><a href="javascript:void(0);">'+ p.name +'</a></h3>\
                    <p><span class="num-orange">'+ p.rank +'</span>'+captions[mode].rank+'</p>\
                    <p><span class="num-blue">'+ p.num_comment +'</span>'+captions[mode].num_comment+'</p>\
            </li>');
			var markerHtml = '<div class="map-mark" data-id="' + poi_id + '">'+order+'</div>';
			var item = $('#poi_' + poi_id);
			item
				.off('mouseenter').on('mouseenter', function() {
					var self = $(this);
					if(self.hasClass('active')) {
						return;
					}
					self.addClass('active');
					var marker_id = self.data('id');
					if(marker_id!=undefined && markers[marker_id] != undefined && markers[marker_id].div != undefined) {
						markers[marker_id].div.find('.map-mark').addClass('on');
					}
				})
				.off('mouseleave').on('mouseleave', function() {
					var self = $(this);
					if(!self.hasClass('active') || (currPop && currPop.id == self.data('id'))) {
						return;
					}
					self.removeClass('active');
					var marker_id = self.data('id');
					if(marker_id!=undefined && markers[marker_id] != undefined && markers[marker_id].div != undefined) {
						markers[marker_id].div.find('.map-mark').removeClass('on');
					}
				})
				.off('click').on('click', function(e) {
					e.preventDefault();
					var self = $(this);
					if(currPop && currPop.id == self.data('id')) {
						return;
					}

					self.addClass('active');
                    var marker_id = self.data('id');
                    if(marker_id!=undefined && markers[marker_id] != undefined && markers[marker_id].div != undefined) {
                        markers[marker_id].div.find('.map-mark').addClass('on');
                        showPOIPop(markers[marker_id]);
                    }
				});

			markers[poi_id] = new marker_handle(map, markerHtml, latlng, function(){
				var that = this;
				var div = that.div;
				div.on('click', function(e) {
					e.preventDefault();
					var self = $(this);
					if(currPop && currPop.id == self.data('id')) {
						return;
					}
					div.find('.map-mark').addClass('on');
					showPOIPop(that);
				});
			});
			markers[poi_id].id = poi_id;
			markers[poi_id].popup_html = '<div class="m-hotelPop" data-id="'+poi_id+'" style="width:240px;">'+
				'<a target="_blank" href="'+captions[mode].link+poi_id+'.html" target="_blank" class="pic">'+
				'<img src="'+p.img_link+'" width="95" height="95">'+
				'</a>'+
				'<h3><a target="_blank" href="'+captions[mode].link+poi_id+'.html">'+ p.name +'</a></h3>'+
				'<p><span class="num-orange">'+ p.rank +'</span>'+captions[mode].rank+'<br />'+
				'<span class="num-blue">'+ p.num_comment +'</span>'+captions[mode].num_comment+'</p>'+
				'<a href="'+captions[mode].link+poi_id+'.html" target="_blank">查看详情&gt;</a></div>'+
				'<a class="m-close" href="javascript:void(0);"></a>'+
				'</div>';
		}
		map.fitBounds(bounds);
	}catch(ex){

	}
}

/* copy form gmap.popup.v2.js by loin */

function showPOIPop(marker) {
	if(currPop) {
		hidePOIPop(currPop.id);
	}
	if(currOtherPop) {
		hideOtherPop(currOtherPop.id);
	}
	marker.popup = new infowin_hanlde({
		map: map,
		text: marker.popup_html,
		latLng: marker.latLng,
		bottom: 34,
		ready: function() {
			var that = this;
			var div = that.div;
			var elem = div.find('.m-hotelPop');
			var close = div.find('.m-close');
			close.on('click', function(e) {
				e.preventDefault();
				hidePOIPop(elem.data('id'));
			})
		}
	});

	currPop = marker.popup;
	//console.log(currPop);
	currPop.id = marker.id;
	allPop[marker.id] = marker.popup;
}

function hidePOIPop(poiid) {
	if(allPop[poiid]!==undefined){
		allPop[poiid].setMap(null);
		currPop = null;
		if(markers[poiid]) {
			var elem = markers[poiid].div.find('.map-mark');
			elem.removeClass('on');
			$('#poi_'+poiid).removeClass('active');
		}
	}
}

function hideOtherPop(id) {
	allOtherPop[id].setMap(null);
	var typeId = '';
	if(currOtherPop) {
		typeId = currOtherPop.type_id;
		currOtherPop = null;
	}

	if(otherMarkers[typeId][id]) {
		var elem = otherMarkers[typeId][id].div.find('.mark-poi');
		elem.removeClass('on');
	}
}

function MarkerOverlay() {
	var Overlay = function(map, text, latLng, ready) {
		this.map = map;
		this.text = text;
		this.latLng = latLng;
		this.ready = ready;
		this.div = null;
		this.setMap(this.map);
	}

	Overlay.prototype = new google.maps.OverlayView();

	$.extend(Overlay.prototype, {
		onAdd: function() {
			var div = $('<div/>').html(this.text).css('position', 'absolute');
			$(this.getPanes().overlayImage).append(div);
			this.div = div;
			if(typeof this.ready === 'function') {
				this.ready.call(this);
			}
		},
		draw: function() {
			var latLng = this.getProjection().fromLatLngToDivPixel(this.latLng);
			this.div.css({
				'left': latLng.x - this.div.width() / 2,
				'bottom': -latLng.y
			});
		},
		onRemove: function() {
			this.div.remove();
			this.div = null;
		}
	});

	return Overlay;
}

function InfoWindowOverlay() {
	var Overlay = function(opt) {
		this.opt = opt;
		this.map = opt;
		this.div = null;
		this.latLng = opt.latLng;
		this.bottom = typeof opt.bottom == 'undefined' ? 0 : parseInt(opt.bottom);
		this.ready = typeof opt.ready == 'undefined' ? '' : opt.ready;
		this.setMap(opt.map);
	}

	Overlay.prototype = new google.maps.OverlayView();

	$.extend(Overlay.prototype, {
		onAdd: function () {
			var div = $('<div/>').html(this.opt.text).css({
				'position': 'absolute',
				'cursor': 'default',
				'z-index': 5
			});
			$(this.getPanes().floatPane).append(div);
			this.div = div;
			if(typeof this.ready === 'function') {
				this.ready.call(this);
			}
		},
		draw: function () {
			var projection = this.getProjection();
			var divPixel = projection.fromLatLngToDivPixel(this.latLng);
			//解决方案一
			var bounds = this.map.getBounds();
			var ne = projection.fromLatLngToDivPixel(bounds.getNorthEast());
			var sw = projection.fromLatLngToDivPixel(bounds.getSouthWest());

			this.div.css({
				'left': parseInt(divPixel.x) - this.div.outerWidth() / 2,
				'bottom': -(divPixel.y - this.bottom)
			});

			if (divPixel.x > ne.x || divPixel.x < sw.x || divPixel.y < ne.y || divPixel.y > sw.y) {
				var c = projection.fromLatLngToDivPixel(this.map.getCenter());
				this.map.panBy(divPixel.x - c.x, divPixel.y - this.div.outerHeight() - ne.y - 110);
			} else {
				var x = 0;
				var y = 0;
				var adjust = 50;
				if (divPixel.x > ne.x - 680) {
					adjust = 110;
				}
				if (divPixel.x < (sw.x + 190 + 15)) {
					x = divPixel.x - (sw.x + 190 + 15);
				}
				if (divPixel.x > (ne.x - 190 - 40)) {
					x = divPixel.x - (ne.x - 190 - 40);
				}
				y = (divPixel.y - (this.div.outerHeight() + adjust)) - ne.y;
				if (x || y) {
					this.map.panBy(x, y < 0 ? y : 0);
				}
			}
			//解决方案二
			/*this.div.css({
			 'left': divPixel.x - 160,
			 'bottom': -(divPixel.y - 34)
			 });
			 this.map.setCenter(this.latLng);*/
		},
		onRemove: function () {
			this.div.remove();
			this.div = null;
		}
	});
	return Overlay;
}
</script>
