            $("._j_hover").hover(function(){
                $(".m-hotAsk ul li").removeClass("on");
                $(this).addClass('on');
            },function(){
                $(".m-hotAsk ul li").removeClass("on");
            });
            $('.book-slide').mfwSlide({
                'width' : 260,
                'height': 180,
                'speed' : 300,  //ÊªöÂä®ÈÄüÂ∫¶
                'item' : 'ul>li', //
                'thumb_box' : '.dot-nav',  //Áº©Áï•ÂõæÔºåÊ≤°ÊúâËØ∑ÁΩÆÁ©∫
                'prev_btn' : '.book-prev',   //Ââç‰∏ÄÂº†ÔºåÊ≤°ÊúâËØ∑ÁΩÆÁ©∫
                'next_btn' : '.book-next',   //‰∏ã‰∏ÄÂº†ÔºåÊ≤°ÊúâËØ∑ÁΩÆÁ©∫
                'thumb_focus_class' : 'on',  // ÂΩìÂâçÊ≠£Âú®ÊòæÁ§∫ÁöÑÂπªÁÅØÂØπÂ∫îÁöÑÁº©Áï•ÂõæÁöÑclass
                'auto_play' : true,  // Ëá™Âä®Êí≠Êîæ
                'interval' : 8,  // ÂπªÁÅØÂàáÊç¢Êó∂Èó¥Èó¥Èöî
                'btn_auto_hide' : true,  // Ëá™Âä®ÈöêËóè‰∏ä‰∏ÄÂº†Ôºå‰∏ã‰∏ÄÂº†ÊåâÈíÆ
                'contain' : true, //thumb_boxÊòØÂê¶ÊòØ$('#slide_box')ÁöÑÂ≠êÂÖÉÁ¥†,Èô§ÁâπÊÆäÊÉÖÂÜµÂ∫îËØ•‰∏∫true
                'always_play' : true
            });
            $('.sales-slide').mfwSlide({
                'width' : 260,
                'height': 180,
                'speed' : 300,  //ÊªöÂä®ÈÄüÂ∫¶
