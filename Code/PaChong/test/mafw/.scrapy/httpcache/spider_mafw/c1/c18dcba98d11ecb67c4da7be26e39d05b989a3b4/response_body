<script>
(function(){
         var traveltab = $('.tn-dropdown');
         traveltab.mouseenter(function(ev) {
                             $(ev.currentTarget).addClass('dropdown-open');
                                     
                 });
         traveltab.mouseleave(function(ev) {
                             $(ev.currentTarget).removeClass('dropdown-open');
                                     
                 });

        /**游记筛选指南*/
        $(".toggle_filter_guide").click(function(){
            $(this).closest('.tn-filterTips').find(">.pop").toggleClass('hide');
        });

         $(".ginfo_filter").click(function(){
                 var attr = {};
                 attr['mddid'] = $(this).data('mddid');
                 attr['f_group'] = $(this).data('group');
                 attr['f_value'] = $(this).data('name');
                 attr['source'] = 'mdd_ginfo';
                 mfwPageEvent('ugc','ginfo_filter',attr);
         });
         $(".ginfo_tag_click").click(function(){
                 var attr = {};
                 attr['mddid'] = $(this).data('mddid');
                 attr['tagid'] = $(this).data('tag');
                 attr['source'] = 'mdd_ginfo';
                 mfwPageEvent('ugc','ginfo_tag_click',attr);
         });
             
 })();
</script>
