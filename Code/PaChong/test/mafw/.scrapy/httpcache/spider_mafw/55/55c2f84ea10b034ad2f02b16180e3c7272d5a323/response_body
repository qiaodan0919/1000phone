    $(function () {
        if(!(location.href.match(/www\.mafengwo\.cn\/hotel/g) ||
           location.href.match(/www\.mafengwo\.cn\/mdd\/$/g)||
            location.href.match(/www\.mafengwo\.cn\/gonglve\/$/g)||
