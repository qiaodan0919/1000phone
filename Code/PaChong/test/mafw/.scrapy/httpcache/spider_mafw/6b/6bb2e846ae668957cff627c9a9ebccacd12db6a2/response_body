    function setPageletBg() {
        var rows = $('#container >.row,#container .pagelet-block >.row'),
                pos = 0, i, len;
        for (i = 0, len = rows.length; i < len; i++) {
            $(rows[i])[i % 2 == 0 ? 'addClass' : 'removeClass']('row-bg');
        }
    }
    setPageletBg();
    M.Event.on('pagelet loaded', function(data){
        setPageletBg();
        data.pagelet.container.find('img.lazy').lazyload({
            threshold : 400
        });
    });
</script>
