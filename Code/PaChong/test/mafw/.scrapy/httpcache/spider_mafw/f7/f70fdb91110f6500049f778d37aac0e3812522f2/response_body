    <div id="pagelet-block-36d4f1afb2fa1d9edc1deee0aad45d76" class="pagelet-block" data-api=":mdd:pagelet:communityApi" data-params="{&quot;iMddid&quot;:&quot;11753&quot;}" data-async="1" data-controller="">        <div class="row row-ask">
