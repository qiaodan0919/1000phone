            amap = {"aMarks":[{"mddname":"\u74e6\u675c\u5179","left":"202px","top":"182px","iconcolor":"icon-b","iconfont":"false","mddid":65448},{"mddname":"\u7279\u91cc\u68ee\u8d1d\u683c","left":"216px","top":"225px","iconcolor":"icon-b","iconfont":"false","mddid":112876},{"mddname":"\u5df4\u5c14\u91c7\u65af","left":"189px","top":"304px","iconcolor":"icon-b","iconfont":"false","mddid":112884},{"mddname":"\u6c99\u6069","left":"197px","top":"143px","iconcolor":"icon-b","iconfont":"false","mddid":112879},{"mddname":"\u9a6c\u5c14\u672c","left":"302px","top":"245px","iconcolor":"icon-b","iconfont":"false","mddid":112885},{"mddname":"\u4f2f\u6717\u80af","left":"232px","top":"64px","iconcolor":"icon-b","iconfont":"false","mddid":143011}],"sTop":"147px","sLeft":"141px","iMddid":11097};
    function setPageletBg() {
        var rows = $('#container >.row,#container .pagelet-block >.row'),
                pos = 0, i, len;
        for (i = 0, len = rows.length; i < len; i++) {
            $(rows[i])[i % 2 == 0 ? 'addClass' : 'removeClass']('row-bg');
        }
    }
    setPageletBg();
    M.Event.on('pagelet loaded', function(data){
        setPageletBg();
        data.pagelet.container.find('img.lazy').lazyload({
            threshold : 400
        });
    });
</script>
